
@{
    ViewBag.Title = "CortesProgramados";
    Layout = "";
}

<div class="col-lg-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5><i class="fa fa-crop"></i> Cortes programados</h5>
             
        </div>
        <div class="ibox-content">

            <ul>
                <li ng-repeat="x in tablecortes" >
                    <h5>{{ x.Titulo }}</h5>
                    <table class="table table-bordered">
                        <tr>
                            <td>Fecha</td>
                            <td>{{ x.Fecha.substring(0, 10) }}</td>
                        </tr>
                        <tr>
                            <td>Hora Inicio</td>
                            <td>{{ x.HoraInicio }}</td>
                        </tr>
                        <tr>
                            <td>Hora de finalización</td>
                            <td>{{ x.HoraFin }}</td>
                        </tr>
                        <tr>
                            <td>Ubicación</td>
                            <td>{{ x.Ubicacion }}</td>
                        </tr>
                        <tr>
                            <td>Descripción</td>
                            <td>{{ x.Descripcion }}</td>
                        </tr>
                    </table>
                    
                </li>
            </ul>
        </div>
    </div>
</div>